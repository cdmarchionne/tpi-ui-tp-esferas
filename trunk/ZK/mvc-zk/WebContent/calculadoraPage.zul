<?page title="Calculadora Web"?>
<window title="Esta es la Calculadora Web" border="normal" width="400px"
	apply="controllers.CalculadoraController">
	<!--  style>"background-image:url(imagenes.calculadora.gif)"</style>-->


	<grid>
		<rows>
			<row>
				Operando1
				<textbox id="op1" constraint="no empty"/>
			</row>
			<row>
				Operando2
				<textbox id="op2" constraint="no empty"/>
			</row>
			<row>
				Resultado:
